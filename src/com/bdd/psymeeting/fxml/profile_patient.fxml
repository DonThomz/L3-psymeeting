                                                                        <JFXComboBox fx:id="jobs_list_field"
                                                                                     prefHeight="26.0"
                                                                                     prefWidth="188.0"/>
                                                                    </children>
                                                                    <VBox.margin>
                                                                        <Insets bottom="20.0" left="20.0" right="20.0"
                                                                                top="20.0"/>
