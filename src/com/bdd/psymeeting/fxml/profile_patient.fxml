                                                                        <Insets bottom="10.0" left="10.0" right="10.0"
                                                                                top="10.0"/>
                                                                    </VBox.margin>
                                                                </HBox>
                                                                <HBox alignment="CENTER_LEFT">
                                                                    <children>
                                                                        <Region prefHeight="16.0" prefWidth="20.0"
                                                                                HBox.hgrow="NEVER"/>
                                                                        <Label styleClass="label_form" text="Métiers"/>
